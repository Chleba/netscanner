      "<Ctrl-z>": "Suspend", // Suspend the application
      "<i>": "InputMode",
      "<g>": "Graph",
      "<d>": "Dump",
      "<f>": "Interface",
      "<s>": "Scan",
      "<up>": "Up",
      "<down>": "Down",
      "<left>": "Left",
      "<right>": "Right",
      "<Tab>": "Tab",
    },
    "Input": {
      "<Esc>": "NormalMode"
    }
  }
}
